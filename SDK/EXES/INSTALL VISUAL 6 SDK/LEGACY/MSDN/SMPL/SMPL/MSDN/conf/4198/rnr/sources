MAJORCOMP=testutil
MINORCOMP=rnrsvc

TARGETNAME=rnrsvc
TARGETPATH=obj
TARGETTYPE=PROGRAM

USE_CRTDLL=1

INCLUDES=.

SOURCES=\
        client.c        \
        connect.c       \
        globals.c       \
        log.c           \
        main.c          \
        rnrsvc.rc       \
        rnrutil.c


UMTYPE=console
UMLIBS=\
        $(BASEDIR)\public\sdk\lib\*\wsock32.lib         \
        $(BASEDIR)\public\sdk\lib\*\user32.lib          \

NTTARGETFILE0=rnrmsg.h

PRECOMPILED_INCLUDE=rnrsvcp.h
