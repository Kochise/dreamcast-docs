                    34
    AUTOCHECKBOX    "Wrap Text", CB_DEFAULT_WRAP, 18, 24, 47, 10
    PUSHBUTTON      "Ok", IDOK, 32, 56, 40, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 88, 56, 40, 14
END

