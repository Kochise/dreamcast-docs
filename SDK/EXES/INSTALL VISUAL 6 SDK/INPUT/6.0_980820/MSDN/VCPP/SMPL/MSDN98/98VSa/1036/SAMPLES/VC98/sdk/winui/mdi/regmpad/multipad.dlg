    EDITTEXT        IDD_SAVETO, 5, 17, 95, 12, ES_AUTOHSCROLL
    LTEXT           "&Directories", -1, 6, 42, 77, 10
    LISTBOX         IDD_DIRS, 5, 55, 90, 50, LBS_SORT | WS_VSCROLL | 
                    WS_TABSTOP
    DEFPUSHBUTTON   "&OK", IDOK, 112, 10, 40, 14, WS_GROUP
    PUSHBUTTON      "&Cancel", SHORTMENU, 112, 30, 40, 14, WS_GROUP
    LTEXT           "", IDD_PATH, 6, 31, 99, 10
END

600 DIALOG 56, 61, 128, 66
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Print"
FONT 8, "Helv"
BEGIN
    RTEXT           "Printing", DLG_INSTALL, 2, 6, 44, 8, NOT WS_GROUP
    LTEXT           "", IDD_PRINTTITLE, 51, 6, 78, 8, NOT WS_GROUP
    RTEXT           "on the", ST_ORGANIZATION, 4, 15, 43, 8, NOT WS_GROUP
    LTEXT           "", IDD_PRINTDEVICE, 51, 15, 78, 8, NOT WS_GROUP
    RTEXT           "on", EB_ORGANIZATION, 21, 24, 26, 8, NOT WS_GROUP
    LTEXT           "", IDD_PRINTPORT, 51, 24, 78, 8, NOT WS_GROUP
    DEFPUSHBUTTON   "Cancel", IDS_CANTOPEN, 75, 46, 47, 14
END

DLG_INSTALL DIALOG 89, 42, 222, 113
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Installing Registry Multipad Demo..."
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT           "Name:", ST_NAME, 28, 8, 22, 8
    LTEXT           "Organization:", ST_ORGANIZATION, 6, 24, 44, 8
    EDITTEXT        EB_NAME, 54, 6, 134, 12, ES_AUTOHSCROLL
    EDITTEXT        EB_ORGANIZATION, 54, 22, 134, 12, ES_AUTOHSCROLL, 
                    0x00000080
    GROUPBOX        "Multipad Global Defaults", GB_DEFAULTS, 47, 42, 146, 34
    AUTOCHECKBOX    "Wrap Text", CB_DEFAULT_WRAP, 59, 57, 47, 10
    DEFPUSHBUTTON   "Install", PB_INSTALL, 63, 93, 40, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 119, 93, 40, 14
END

DLG_PER_USER_CONFIG DIALOG 148, 39, 160, 76
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Options"
FONT 8, "MS Sans Serif"
BEGIN
