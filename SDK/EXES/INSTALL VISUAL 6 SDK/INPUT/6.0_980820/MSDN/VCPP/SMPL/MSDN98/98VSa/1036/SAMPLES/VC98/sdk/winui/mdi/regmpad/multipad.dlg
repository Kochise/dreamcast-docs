1 DLGINCLUDE "multipad.h"


LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US

200 DIALOG 20, 20, 204, 118
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "File Open"
FONT 8, "Helv"
BEGIN
    LTEXT           "Open File &Name ", -1, 8, 6, 65, 12
    EDITTEXT        IDD_FILENAME, 74, 6, 80, 12, ES_AUTOHSCROLL
    LTEXT           "Path", IDD_PATH, 74, 24, 82, 10
    LTEXT           "&Files", -1, 6, 23, 55, 10
    LISTBOX         IDD_FILES, 5, 34, 61, 74, LBS_SORT | WS_VSCROLL | 
                    WS_TABSTOP
    LTEXT           "&Directories", -1, 74, 37, 58, 10
    LISTBOX         IDD_DIRS, 73, 50, 61, 58, LBS_SORT | WS_VSCROLL | 
                    WS_TABSTOP
    DEFPUSHBUTTON   "OK", IDOK, 160, 6, 40, 14, WS_GROUP
    PUSHBUTTON      "Cancel", IDCANCEL, 160, 26, 40, 14, WS_GROUP
END

300 DIALOG 126, 29, 190, 131
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "About RegMpad"
FONT 8, "Helv"
BEGIN
    CTEXT           "Microsoft Windows", -1, 6, 5, 184, 8
    CTEXT           "Multipad with Registry State", -1, 6, 15, 184, 8
    CTEXT           "Version 3.00", -1, 6, 34, 184, 8
    CTEXT           "Copyright ｩ 1993 Microsoft Corp.", -1, 6, 47, 184, 9
    DEFPUSHBUTTON   "OK", IDOK, 82, 111, 32, 14, WS_GROUP
    ICON            IDS_CANTOPEN, -1, 31, 14, 16, 21
    LTEXT           "Installed by:", ST_INSTALLED_BY, 6, 61, 41, 8
    EDITTEXT        EB_NAME, 49, 60, 134, 12, ES_AUTOHSCROLL | ES_READONLY
    EDITTEXT        EB_ORGANIZATION, 49, 79, 134, 12, ES_AUTOHSCROLL | 
                    ES_READONLY, 0x00000880
END

400 DIALOG 18, 13, 167, 69
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Find"
FONT 8, "Helv"
BEGIN
    RTEXT           "&Find:", DLG_INSTALL, 6, 12, 31, 10, NOT WS_GROUP
    EDITTEXT        IDD_SEARCH, 43, 11, 114, 12
    CHECKBOX        "&Case Sensitive", IDD_CASE, 19, 28, 137, 12
    DEFPUSHBUTTON   "&Next", IDS_CANTOPEN, 11, 48, 45, 14
    PUSHBUTTON      "&Previous", IDD_PREV, 63, 48, 45, 14
    PUSHBUTTON      "C&ancel", SHORTMENU, 116, 48, 43, 14
END

500 DIALOG 29, 19, 169, 116
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Save As"
FONT 8, "Helv"
BEGIN
