///////////////////////////////////////////////////////////
//
//	TangramSize2d - Two dimensional size using doubles.
//
typedef struct tagTangramSize2d_ 
{
	double cx ;
	double cy ;
} TangramSize2d;

///////////////////////////////////////////////////////////
//
//	TangramPoin2d - Two dimensional point using doubles.
//
typedef struct tagTangramPoint2d_
{
	double x ;
	double y ;
} TangramPoint2d;
